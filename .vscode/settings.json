{
  "editor.formatOnSave": true,
  // "prettier.eslintIntegration": true,
  "movets.skipWarning": true,
  "typescriptHero.imports.organizeOnSave": true,
  "typescriptHero.imports.multiLineTrailingComma": false,
  "typescriptHero.imports.insertSpaceBeforeAndAfterImportBraces": false,
  "typescriptHero.imports.multiLineWrapThreshold": 110,
  "files.eol": "\n",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html"
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    ".cache-loader": true,
    "node_modules": true,
    "coverage": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/coverage": true
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact",
    "markdown": "javascriptreact"
  },
  "emmet.excludeLanguages": [],
  "beautify.ignore": ["*"],
  "[html]": {
    "editor.formatOnSave": false,
    "editor.formatOnPaste": false
  },
  "editor.formatOnSaveTimeout": 5000,
  "typescript.preferences.renameShorthandProperties": false
}
